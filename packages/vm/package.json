{
  "name": "@aldea/vm",
  "description": "The Aldea Computer virtual machine implemented in JavaScript.",
  "version": "0.3.6",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "build/**/*",
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "test": "ts-mocha test/**/*.spec.ts",
    "build": "yarn clean && yarn build:asc && yarn build:tsc && yarn build:es",
    "build:tsc": "tsc",
    "build:asc": "node cmd/compile-all.js",
    "build:es": "node esbuild.js",
    "ci": "yarn test",
    "dev": "node cmd/watch.js",
    "fileId": "cmd/calculate-package-id.js",
    "clean": "rm -r dist/* build/* src/builtins/*.ts || true"
  },
  "packageManager": "yarn@3.2.3",
  "dependencies": {
    "@aldea/compiler": "workspace:^",
    "@aldea/core": "workspace:^",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "assemblyscript": "npm:@aldea/assemblyscript@^0.1.0",
    "chai": "^4.3.7",
    "chokidar": "^3.5.3",
    "esbuild": "^0.17.19",
    "eslint": "^8.40.0",
    "glob": "^10.2.4",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
