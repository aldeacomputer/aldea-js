{
  "name": "@aldea/sdk-js",
  "version": "0.0.1-beta.2",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/aldea.bundle.mjs",
      "require": "./dist/aldea.bundle.cjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/aldea.bundle.mjs"
    },
    "./*": {
      "import": "./dist/*.js",
      "node": "./dist/*.js",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "main": "./dist/aldea.bundle.mjs",
  "module": "./dist/aldea.bundle.mjs",
  "browser": "./dist/aldea.bundle.min.js",
  "unpkg": "./dist/aldea.bundle.min.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/aldea.bundle.cjs",
    "dist/aldea.bundle.mjs",
    "dist/aldea.bundle.min.js",
    "dist/aldea.bundle.min.js.map",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "build": "run-s clean build:ts build:es",
    "build:ts": "tsc",
    "build:es": "node esbuild.js",
    "ci": "yarn test",
    "clean": "rimraf dist/* || true",
    "docs": "typedoc",
    "test": "run-s build test:browser && ava",
    "test:browser": "playwright test",
    "test:local": "ava",
    "vectors": "node scripts/vectorgen.js"
  },
  "packageManager": "yarn@3.2.3",
  "dependencies": {
    "@aldea/compiler": "^0.0.0-alpha",
    "@noble/ed25519": "^1.7.1",
    "@noble/hashes": "^1.1.3",
    "@scure/base": "^1.1.1",
    "@scure/bip39": "^1.1.0",
    "cbor-redux": "^1.0.0-3",
    "cross-fetch": "^3.1.5",
    "ky": "^0.33.1",
    "ky-universal": "^0.11.0",
    "micro-ed25519-hdkey": "^0.1.2"
  },
  "devDependencies": {
    "@mxssfd/typedoc-theme": "^1.0.3",
    "@playwright/test": "^1.29.1",
    "ava": "^4.3.3",
    "esbuild": "^0.15.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typedoc": "^0.23.23",
    "typescript": "^4.8.4"
  },
  "ava": {
    "concurrency": 4,
    "files": [
      "test/**/*.test.js",
      "!test/browser"
    ]
  }
}
